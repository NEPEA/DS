#include <stdio.h>
void main()
{   int a,n,i,arr[20];
    printf("how many elements you want to enter in the array \n");
    scanf("%d",&n);
    printf("eneter the elements of array\n");
    for (i=0;i<=n-1;i++)
        scanf("%d",&arr[i]);
        printf("eneter the position at which you want to delete the data element\n");
        scanf("%d",&a);
    printf("afetr deleting the element of array at kth location \n");
    for (i=a-1;i<=n;i++)
     {arr[i]=arr[i+1];}
     for (i=0;i<n-1;i++)
      printf("%d\t",arr[i]);
      getch();
}

