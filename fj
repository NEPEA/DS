    for (i=0;i<=n-1;i++)
        scanf("%d",&arr[i]);
    printf("afetr deleting the first element of array\n");
    for (i=0;i<=n;i++)
     {arr[i]=arr[i+1];}
     for (i=0;i<n-1;i++)
      printf("%d\t",arr[i]);
